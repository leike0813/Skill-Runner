{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "x": {
            "type": "integer"
        },
        "y": {
            "type": "integer"
        },
        "numbers": {
            "type": "array",
            "items": {
                "type": "integer"
            },
            "minItems": 2,
            "maxItems": 2
        },
        "comparison": {
            "type": "string",
            "enum": [
                "ge",
                "le",
                "eq"
            ]
        },
        "parity": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "even",
                    "odd"
                ]
            },
            "minItems": 2,
            "maxItems": 2
        },
        "is_prime": {
            "type": "array",
            "items": {
                "type": "boolean"
            },
            "minItems": 2,
            "maxItems": 2
        },
        "is_3x": {
            "type": "array",
            "items": {
                "type": "boolean"
            },
            "minItems": 2,
            "maxItems": 2
        },
        "generated_at": {
            "type": "string",
            "description": "ISO datetime string generated by the skill."
        }
    },
    "required": [
        "x",
        "y",
        "numbers",
        "comparison",
        "parity",
        "is_prime",
        "is_3x",
        "generated_at"
    ],
    "additionalProperties": false
}
